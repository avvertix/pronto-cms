{
    "name": "avvertix/pronto-cms",
    "description": "Pronto CMS. The CMS that is almost pronto (ready in italian) for you and your website.",
    "keywords": ["cms","markdown","flat-file", "site", "laravel", "lumen"],
    "license": "GPL-3.0",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/lumen-framework": "5.1.*",
        "vlucas/phpdotenv": "~1.0",

        "intervention/image": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Pronto\\": "pronto-framework/src"
        },
        "files": [
            "pronto-framework/src/helpers.php"
        ],
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
