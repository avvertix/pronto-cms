{
    "name": "avvertix/pronto",
    "description": "Pronto CMS. The CMS that is almost pronto (ready in italian) for you and your website.",
    "keywords": ["cms","markdown","flat-file", "site", "laravel", "lumen"],
    "license": "GPL-3.0",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "avvertix/pronto-framework": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist"
        
    },
    "minimum-stability":"stable"
    
}
